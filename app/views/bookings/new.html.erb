

<h1>Book <%= @listing.title %></h1>

<%= simple_form_for [@listing, @booking], data: {controller: "flatpickr"} do |f| %>

  <%= f.input :start_date, as: :string, input_html: { data: { flatpickr_target: "startDate" } } %>
  <%= f.input :end_date, as: :string, input_html: { data: { flatpickr_target: "endDate" } } %>

  <%= f.submit 'Book Listing'%>
<% end %>

<%= link_to 'Back', listing_path(@listing)%>


